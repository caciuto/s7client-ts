{
  "name": "s7client-ts",
  "version": "0.0.2",
  "description": "Step7 client based on node-step7 and inspired by s7client, Typescript ready",
  "keywords": [
    "step7",
    "s7",
    "plc",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --watch 'src/**' --ext 'ts' --exec ts-node ./src/index.ts",
    "build": "npm run build:clean; npm run build:compile",
    "build:clean": "rm -rf dist/*",
    "build:compile": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Alberto Brino <alberto.brino@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "lodash.random": "^3.2.0",
    "node-snap7": "^1.0.5"
  },
  "devDependencies": {
    "@types/lodash.random": "^3.2.6",
    "@types/node": "^14.14.22",
    "@types/node-snap7": "^0.2.19",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
